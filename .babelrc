{
  "presets": ["es2015", "react"],
  "retainLines": "true",
  "plugins": [
    "transform-es2015-typeof-symbol",
    "transform-class-properties",
    "add-module-exports",
    "transform-object-rest-spread",
    ['module-resolver', {
      alias: {
        client: './app/client',
        shared: './app/shared',
        server: './app/server'
      }
    }]
  ]
}
